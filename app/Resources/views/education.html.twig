<div class="education-history col-md-6 padding_15 padbot_30">
    <ul class="timeline col-md-12 top_30">
        <li><i class="fa fa-graduation-cap" aria-hidden="true"></i><h2 class="timeline-title">Education History</h2></li>
        <!-- a work -->
        {% for education in educations %}
            <li><h3 class="line-title">{{ education.typeofeducaion }}</h3>
                <span>{% if education.dateDeb %}{{ education.dateDeb|date('Y-m-d') }}{% endif %} - {% if education.dateFin %}{{ education.dateFin|date('Y-m-d') }}{% endif %}</span>
                <p class="little-text">{{ education.descrption }}</p>
            </li>
        {% endfor %}
    </ul>
</div>